REM 
REM READ MUSIC DATA
 T$="EL CONDR PASA?"
 DIM M(1000)
 RESTORE MDATA
 FOR I=0 TO 999
  M(I)=READ()
  IF M(I) = 0 THEN BREAK
 NEXT
REM BUILD MUSIC SCALE
 DIM A(24)
 A(0)=0
 FOR I=1 TO 11: A(I)=A(I-1)+I/3+1:NEXT
 FOR I=12 TO 24: A(I)=A(I-1)+I-7:NEXT
REM A#3=30
 B=30
REM A4=41(B+I)
 I=11
REM MAIN
 WHILE 1
  CLS
  PWM 41,125000,1
  GPRINT 24,110,7,0,"tune OTAMAMIN as A then HIT START"
  WHILE KEYS(16)=0:WAIT 1:WEND
  CLS
  GPRINT 24,110,7,0,T$
  FOR I=0 TO 999
   IF M(I) == 0 THEN 
    BREAK
   ELSEIF M(I) > 70 THEN
    WAIT (M(I)-70)*30
   ELSE
    PWM A(M(I))+B,125000,1:WAIT 30
   ENDIF
  NEXT
 WEND
LABEL MDATA
